*{-webkit-user-select:none;-webkit-tap-highlight-color:rgba(255,255,255,0)}
*{margin:0;padding:0}
img{border:0}
body{overflow-x:hidden;font-family:Arial,Helvetica,sans-serif;color:#fff;font-size:14px;display:-webkit-box}
.panel{width:100%;height:100%;position:absolute;top:0;left:0;overflow:hidden;-webkit-perspective:1000;background:url(../images/background.png) top left repeat}
.arrow{position:absolute;width:32px;height:32px;top:50%;margin-top:-16px;right:3%}
.rlImg{position:relative; float:left; height:40%; left:3%; top:30%;}
.title{width:100%;height:46px;text-align:left;color:#556a76;font:bold 18px "Helvetica Neue",Arial,Helvetica,Geneva,sans-serif;position:relative;padding-left:19px;-webkit-box-shadow:0 0px 1px 0px rgba(201,218,222,1)}
.title p{position:relative;top:13px;}
.content{width:100%; display:none;-webkit-border-radius:6px; position:relative;}
#addRight{right:6%}
.trans{opacity:0.6; width:100%; height:100%; background-color:#e0f3f7; -webkit-border-radius:6px; position:absolute; z-index:0}
ul{width:100%; height:auto; padding:0 0 0 0; margin:0 0 0 0; list-style:none; display:block; position:relative;}
li{ -webkit-box-sizing:border-box; float:left; font-size:16px; color:#333333; border-top:dashed 1px #cccccc; border-right:dashed 1px #cccccc; overflow:hidden; display:list-item;}
.ul3 li{width:33.3%; height:37px; line-height:37px;}
.ul3 li:nth-child(3n){border-right:0px;}
.ul3 li:nth-child(-n+3){border-top:0px;}
.ul4 li{width:25%; height:37px; line-height:37px;}
.ul4 li:nth-child(4n){border-right:0px;}
.ul4 li:nth-child(-n+4){border-top:0px;}
.ul5 li{width:20%; height:37px; line-height:37px;}
.ul5 li:nth-child(5n){border-right:0px;}
.ul5 li:nth-child(-n+5){border-top:0px;}
.ul5 li:nth-child(5n+1){color:#2eaccc;}
#ulImg{padding:4%}
#ulImg li{width:24%; vertical-align:bottom; border:0px; font-size:12px; padding-right:3.3%; padding-bottom:10px;}
#ulImg li:nth-child(-n+4){padding-top:0px}
#ulImg li img{width:58px; height:58px; clear:both;}
@media all and (orientation:portrait){
.page{display:block;position:relative;float:left;text-align:center;width:50%;height:100%;z-index:10;}
.pageTitle{text-align:left;left:4%;font:bold 18px "Helvetica Neue",Arial,Helvetica,Geneva,sans-serif;width:96%;height:11%;color:#328097;position:relative;padding-top:5%;overflow:hidden}
.titleIcon{float:right;height:25px;width:30px;background-size:17px 17px}
.line{display:inline;float:right;margin-right:13px;height:17px;width:1px}
.urlContainer{width:100%; height:79%; /*height:62%;*/ position:relative;overflow:hidden; /*top:18%;*/}
.iconContainer{width:20%;height:84px;padding-top:2px;display:block;float:left;position:relative;margin-left:3.5%;margin-bottom:10px;-webkit-tap-highlight-color:rgba(128,128,128,0.5)}
.iconContainer:nth-child(4n+1){margin-left:5%}
.iconContainer span{clear:both;width:100%;padding-top:2px;height:14px;display:block;font:bold 11px "Helvetica Neue",Arial,Helvetica,Geneva,sans-serif;color:#666666;position:absolute;vertical-align:middle;top:72px;overflow-x:hidden;text-overflow:ellipsis;white-space:nowrap}
.iconImage{position:relative;width:64px;height:64px;left:50%;margin-left:-32px}
.iconImage img{display:block;width:64px;height:64px;position:absolute;padding-left:auto;padding-right:auto;-webkit-border-radius:5px}
.add{position:relative;width:64px;height:64px;left:50%;margin-left:-32px}
.add img{display:block;width:64px;height:64px;position:absolute;-webkit-border-radius:5px;padding-left:auto;padding-right:auto}
.mostVisited{overflow:hidden;width:90%;height:79%;position:relative;background-color:rgba(252,252,252,0.5);left:5%;-webkit-border-radius:6px;-webkit-box-shadow:0 0 1px 1px rgba(86,116,127,0.3);top:-10px}
.mvItem{width:100%;height:20%;position:relative;background:url(content://com.orange.assetfile/html/images/mvLine.png) bottom left no-repeat;background-position:bottom}
.mvItem .mvSpan{display:block;position:relative;font:normal 18px "Helvetica Neue",Arial,Helvetica,Geneva,sans-serif;width:80%;height:40%;color:#556a76;text-align:left;left:5%;padding-top:2px;top:15%;overflow-x:hidden;text-overflow:ellipsis;white-space:nowrap}
.mvItem .mvSpan:nth-child(2n){font-size:12px;color:#999999;font-weight:normal;top:12%;text-overflow:ellipsis;white-space:nowrap}
.mvItem .rlSpan{display:block;position:relative;float:left; font:normal 18px "Helvetica Neue",Arial,Helvetica,Geneva,sans-serif; width:70%; height:40%; color:#556a76;text-align:left;left:5%;padding-top:4px; top:30%;overflow-x:hidden;text-overflow:ellipsis;white-space:nowrap;}
.mvItem .blank{color:#999999;top:30%}
#carousel_dots{width:100%;height:6%;padding-left:41%;position:fixed;bottom:0;vertical-align:center;z-index:100}
#carousel_0{width:7px;height:100%;float:left;position:relative;background-size:100% 8px}
#carousel_1{width:7px;height:100%;position:relative;float:left;left:20px;background-size:100% 8px}
#carousel_2{width:7px;height:100%;position:relative;left:54px;background-size:100% 8px}
.carousel_paging_selected{background:url(content://com.orange.assetfile/html/images/dotSelected.png) center center no-repeat}
.carousel_paging{background:url(content://com.orange.assetfile/html/images/dot.png) center center no-repeat}
#siteNavContainer{overflow:hidden;width:90%;max-height:79%;min-height:230px;height:auto;top:-10px;position:relative;background-color:rgba(252,252,252,0.5);left:5%;-webkit-border-radius:6px;-webkit-box-shadow:0 0 1px 1px rgba(86,116,127,0.3)}
.medalRanking{width:100%; position:relative; display:block;}
#medalBg{width:100%; position:absolute;}
.medalNum{position:absolute; display:block;}
.medalNum img{height:100%; display:block;}
}
@media all and (orientation:landscape){
.page{display:block;position:absoulte;text-align:center;width:50%;height:100%;z-index:10;}
.pageTitle{display:block;text-align:left;left:3%;font:bold 18px "Helvetica Neue",Arial,Helvetica,Geneva,sans-serif;width:97%;height:43px;color:#328097;position:relative;padding-top:3%;overflow:hidden;}
.titleIcon{float:right;height:25px;width:30px;background-size:17px 17px}
.line{display:inline;float:right;margin-right:13px;height:17px;width:1px}
.urlContainer{width:100%;height:62%;position:relative;overflow:hidden;padding-top:2px}
.iconContainer{width:13%;height:84px;float:left;position:relative;margin-left:3.3%;margin-bottom:10px;-webkit-tap-highlight-color:rgba(128,128,128,0.5)}
.iconContainer span{clear:both;padding-top:2px;width:100%;height:16px;display:block;text-align:center;font:bold 11px "Helvetica Neue",Arial,Helvetica,Geneva,sans-serif;color:#666666;position:absolute;vertical-align:middle;top:71px;overflow-x:hidden;text-overflow:ellipsis;white-space:nowrap}
.iconImage{position:relative;width:64px;height:64px;left:50%;margin-left:-32px}
.iconImage img{position:absolute;height:64px;width:64px;padding-left:auto;padding-right:auto;-webkit-border-radius:5px}
.add{position:relative;width:64px;height:64px;left:50%;margin-left:-32px}
.add img{display:block;width:64px;height:64px;position:absolute;-webkit-border-radius:5px;padding-left:auto;padding-right:auto}
.mostVisited{overflow:hidden;width:94%;height:66%;position:relative;background-color:rgba(252,252,252,0.5);left:3%;-webkit-border-radius:6px;top:-13px;-webkit-box-shadow:0 0 1px 1px rgba(86,116,127,0.3);padding-top:1%}
.mvItem{width:100%;height:53px;position:relative;background:url(content://com.orange.assetfile/html/images/mvLine.png) bottom left repeat-x;background-position:bottom}
.mvItem .mvSpan{display:block;position:relative;font:normal 18px "Helvetica Neue",Arial,Helvetica,Geneva,sans-serif;width:85%;height:40%;color:#556a76;text-align:left;left:5%;top:15%;padding-top:2px;overflow-x:hidden;text-overflow:ellipsis;white-space:nowrap}
.mvItem .mvSpan:nth-child(2n){font-size:12px;color:#999999;font-weight:normal;top:12%;text-overflow:ellipsis;white-space:nowrap}
.mvItem .rlSpan{display:block;position:relative;float:left;font:normal 18px "Helvetica Neue",Arial,Helvetica,Geneva,sans-serif;width:85%; height:40%;color:#556a76;text-align:left;left:5%;top:30%; overflow-x:hidden;text-overflow:ellipsis;white-space:nowrap}
.mvItem .blank{color:#999999;top:30%}
#carousel_dots{width:100%;height:12px;padding-left:43%;position:fixed;bottom:0}
#carousel_0{width:7px;height:100%;float:left;position:relative;background-size:100% 8px}
#carousel_1{width:7px;height:100%;position:relative;float:left;left:20px;background-size:100% 8px}
#carousel_2{width:7px;height:100%;position:relative;left:54px;background-size:100% 8px}
.carousel_paging_selected{background:url(content://com.orange.assetfile/html/images/dotSelected.png) top left no-repeat}
.carousel_paging{background:url(content://com.orange.assetfile/html/images/dot.png) top left no-repeat}
#siteNavContainer{overflow:hidden;width:94%;max-height:66%;height:auto;top:-10px;position:relative;background-color:rgba(252,252,252,0.5);left:3%;-webkit-border-radius:6px;-webkit-box-shadow:0 0 1px 0px rgba(86,116,127,0.3)}
.medalRanking{display:none;}
.medalRanking img{display:none;}
.medalNum{display:none;}
}
.wordLink:link{display:block; width:100%; height:100%; line-height:37px; text-decoration:none; text-align:center; color:#333333;}
.wordLink:visited{display:block; width:100%; height:100%; line-height:37px; text-decoration:none; text-align:center; color:#333333;}
.wordLink:hover{display:block; width:100%; height:100%; line-height:37px; text-decoration:none; text-align:center; color:#333333; /*background-color:#2eaccc;*/}
.wordLink:active{display:block; width:100%; height:100%; line-height:37px; text-decoration:none; text-align:center; color:#333333;}