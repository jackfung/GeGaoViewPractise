<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@color/gray"
    android:orientation="horizontal" >

    <ImageView
        android:id="@+id/bottom_button_backward"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:scaleType="centerInside"
        android:src="@drawable/btn_backward_regist" />
    
    <ImageView
        android:id="@+id/bottom_button_forward"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:scaleType="centerInside"
        android:src="@drawable/btn_forward_regist" />

  <!--   <ImageView
        android:id="@+id/bottom_button_read"
        android:layout_width="fill_parent"
        android:layout_height="46dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:src="@drawable/bt_read"
        android:scaleType="centerInside" /> -->
    <ImageView
        android:id="@+id/bottom_button_function"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:src="@drawable/btn_more_regist"
        android:scaleType="centerInside" />
         

    <ImageView
        android:id="@+id/bottom_button_home"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:src="@drawable/btn_home_regist"
        android:scaleType="centerInside" />
    
    <ImageView
        android:id="@+id/bottom_button_window"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:src="@drawable/btn_window_regist"
        android:scaleType="centerInside" />

    <!--  
    <com.orange.browser.TextImageView
